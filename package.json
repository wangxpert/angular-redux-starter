{
  "name": "angular-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for Angular/Redux applications",
  "license": "MIT",
  "keywords": [
    "angular",
    "redux",
    "typescript"
  ],
  "engines": {
    "node": "6"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/",
    "cover": "istanbul report --include=**/coverage/coverage.json text text-summary",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon server/node-server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --debug --inline --hot --progress --no-info",
    "dev": "concurrently -r 'npm run dev:client' 'npm run dev:server'",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-ts": "tslint './src/**/*.ts'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
    "lint-css": "stylelint './src/**/*.css'",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "pretest": "rimraf coverage/ & npm run lint",
    "test": "karma start --singleRun true",
    "posttest": "npm run cover",
    "test:watch": "karma start --singleRun false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/anglar-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/rangle-starter/issues"
  },
  "dependencies": {
    "express": "^4.13.4",
    "helmet": "^2.1.0",
    "http-proxy": "^1.14.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@types/angular-mocks": "^1.5.9",
    "@types/angularjs": "^1.5.14-alpha",
    "@types/core-js": "0.9.34",
    "@types/jasmine": "2.5.35",
    "@types/jquery": "^2.0.40",
    "@types/node": "6.0.45",
    "@types/ramda": "0.0.2",
    "angular": "^1.6.2",
    "angular-mocks": "^1.6.2",
    "autoprefixer": "6.5.1",
    "autoprefixer-core": "^6.0.1",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "body-parser": "^1.15.0",
    "concurrently": "^2.1.0",
    "copy-webpack-plugin": "4.0.0",
    "core-js": "^2.4.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "denodeify": "^1.2.1",
    "eslint": "^3.2.2",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "2.22.0",
    "immutable": "^3.7.6",
    "istanbul": "^1.0.0-alpha.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.5.2 ",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-remap-coverage": "0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "2.0.2",
    "ng-classified": "^1.0.2",
    "ng-redux": "^3.3.3",
    "ngcomponentrouter": "^2.0.0",
    "nodemon": "^1.9.2",
    "null-loader": "^0.1.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-modules-local-by-default": "^1.1.1",
    "ramda": "^0.22.1",
    "raw-loader": "^0.5.1",
    "redux": "^3.5.2",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "remap-istanbul": "^0.6.3",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.0",
    "stylelint-webpack-plugin": "^0.5.1",
    "tachyons": "^4.6.1",
    "ts-helpers": "^1.1.2",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.1.4",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0",
    "webpack-hot-middleware": "^2.10.0"
  }
}
